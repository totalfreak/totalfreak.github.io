{
  "manifest_version": 2,

  "name": "Sanctity",
  "description": "Replaces the new tab",
  "version": "0.3",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://source.unsplash.com/1920x1080/?stars"
  ],
  "web_accessible_resources" : [
    "https://images.unsplash.com/"
  ]
}
