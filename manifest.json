{
    "manifest_version": 2,

    "name": "GachiMuchi",

    "version": "1.0.9",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"

    },
    "permissions": [
    "activeTab",
    "tabs", "http://*/*", "https://*/*"
  ],
    "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.min.js", "script.js"]
    }
  ]

}